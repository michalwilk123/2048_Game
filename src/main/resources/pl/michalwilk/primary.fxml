<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.michalwilk.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label layoutX="112.0" layoutY="21.0" prefHeight="123.0" prefWidth="236.0" text="2048">
               <font>
                  <Font name="Lato Heavy" size="96.0" />
               </font>
            </Label>
            <Label layoutX="270.0" layoutY="144.0" text="by michalwilk123">
               <font>
                  <Font name="Lato Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="300.0" prefWidth="460.0">
         <children>
            <Button layoutX="30.0" layoutY="14.0" mnemonicParsing="false" onAction="#new_game_clicked" prefHeight="100.0" prefWidth="400.0" text="New Game">
               <font>
                  <Font name="Lato Bold" size="29.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="continue_game_button" layoutX="30.0" layoutY="150.0" mnemonicParsing="false" onAction="#continue_game_clicked" prefHeight="100.0" prefWidth="400.0" text="Continue Game">
               <font>
                  <Font name="Lato Bold" size="29.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
