<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.michalwilk.GameOverController">
   <children>
      <AnchorPane prefHeight="264.0" prefWidth="500.0">
         <children>
            <Label layoutX="102.0" layoutY="89.0" text="YOUR SCORE: " />
            <Label layoutX="102.0" layoutY="14.0" text="GAME OVER">
               <font>
                  <Font name="Lato Bold" size="50.0" />
               </font></Label>
            <Button layoutX="263.0" layoutY="172.0" mnemonicParsing="false" prefHeight="78.0" prefWidth="223.0" text="EXIT">
               <font>
                  <Font name="Lato Bold" size="21.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button layoutX="14.0" layoutY="172.0" mnemonicParsing="false" prefHeight="78.0" prefWidth="223.0" text="RESTART">
               <font>
                  <Font name="Lato Bold" size="21.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
